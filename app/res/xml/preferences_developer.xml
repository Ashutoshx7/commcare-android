<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
                  xmlns:app="http://schemas.android.com/apk/res-auto">
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-superuser-enabled"
        android:title="Developer Mode Enabled"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="no"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-show-update-target-options"
        android:title="Show Update Options Item"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="yes"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-action-nav-enabled"
        android:title="Action Bar Enabled"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="yes"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-nav-ui-enabled"
        android:title="Navigation UI"/>
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-list-refresh"
        android:title="Entity List Screen Auto-Refresh"/>
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-enable-auto-login"
        android:title="Auto-login While Debugging"/>
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-enable-session-saving"
        android:title="Enable Session Saving"/>
    <EditTextPreference
        android:dialogTitle="Edit Saved Session"
        android:enabled="true"
        android:key="__edit_session_save"
        android:title="Edit Saved Session"/>
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-css-enabled"
        android:title="CSS Enabled"/>
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-markdown-enabled"
        android:title="Markdown Enabled"/>
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-alternate-question-text-format"
        android:title="Image Above Question Text Enabled"/>
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-home-report"
        android:title="Home Report Button enabled"/>
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-offer-pin-for-login"
        android:title="Give option to use PIN for login"/>
    <android.support.v7.preference.ListPreference
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-auto-purge"
        android:title="Detect and Trigger Purge on Form Save"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="saved"
        android:enabled="true"
        android:entries="@array/pref_load_form_payload_as_labels"
        android:entryValues="@array/pref_load_form_payload_as_vals"
        android:key="cc-form-payload-status"
        android:title="Load form payload status"/>
    <org.commcare.preferences.FilePreference
        android:defaultValue=""
        android:enabled="true"
        android:key="cc-local-form-payload-file-path"
        android:title="Local Form Payload File"
        app:file_type="xml"/>
    <EditTextPreference
        android:defaultValue="@string/remote_form_payload_url"
        android:enabled="true"
        android:key="remote-form-payload-url"
        android:title="Form Payload Server"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="yes"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-detail-final-swipe-enabled"
        android:title="Enable swipe in/out of case detail tabs"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="no"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-use-root-menu-as-home-screen"
        android:title="Use root module menu as home screen"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="no"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-show-entity-trace-outputs"
        android:title="Output Entity List Trace Data to ADB"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="no"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-use-pw-obfuscation"
        android:title="Use Password Obfuscation"/>
    <org.commcare.preferences.FilePreference
        android:defaultValue=""
        android:enabled="true"
        android:key="cc-custom-restore-doc-location"
        android:title="Restore a custom user XML data file"
        app:file_type="xml"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="no"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-enable-bulk-performance"
        android:title="Enable cutting-edge performance improvements"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="no"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-hide-issue-report"
        android:title="Hide the 'Report an Issue' Menu Option"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="no"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-enforce-secure-endpoint"
        android:title="Enforce Secure Endpoint"/>
    <android.support.v7.preference.ListPreference
        android:defaultValue="yes"
        android:enabled="true"
        android:entries="@array/pref_enabled_labels"
        android:entryValues="@array/pref_enabled_vals"
        android:key="cc-allow-space-in-select-choices"
        android:title="Allow Space in Select Choices"/>
</PreferenceScreen>
