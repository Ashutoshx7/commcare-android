					<welcome/>
					<record_or_view/>
					<lmp/>
					<days_long/>
					<predict_next_cycle/>
					<last_lmp/>
					<ovulating/>
					<next_period/>
					<prev_lmp/>
					<view_est_next/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><lmp/><next_period/><prev_lmp/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/><instance id="casedb" src="jr://instance/casedb"/>
			<bind nodeset="/data/welcome"/>
			<bind nodeset="/data/record_or_view"/>
			<bind nodeset="/data/lmp" type="xsd:date" constraint=". &lt; today()" jr:constraintMsg="jr:itext('lmp-constraintMsg')" relevant="/data/record_or_view = 'record_new'"/>
			<bind nodeset="/data/days_long" type="xsd:int" relevant="/data/record_or_view = 'record_new'"/>
			<bind nodeset="/data/predict_next_cycle" relevant="/data/record_or_view = 'record_new'"/>
			<bind nodeset="/data/last_lmp" relevant="/data/record_or_view = 'previous_cycle'"/>
			<bind nodeset="/data/ovulating" relevant="/data/lmp &gt;= date(today() + 11) and /data/lmp &lt;= date(today() + 18)"/>
			<bind nodeset="/data/next_period" calculate="date(/data/lmp + 28)"/>
			<bind nodeset="/data/prev_lmp"/>
			<bind nodeset="/data/view_est_next" calculate="date(date(/data/prev_lmp) + 28)"/>
			<itext>
				<translation lang="hin" default="">
					<text id="welcome-label">
						<value>&#2350;&#2366;&#2361;&#2357;&#2366;&#2352;&#2368; &#2335;&#2381;&#2352;&#2376;&#2325;&#2352; &#2350;&#2375;&#2306; &#2310;&#2346;&#2325;&#2366; &#2360;&#2381;&#2357;&#2366;&#2327;&#2340; &#2361;&#2376; | &#2351;&#2361; &#2347;&#2377;&#2352;&#2381;&#2350; &#2310;&#2346;&#2325;&#2368; &#2350;&#2366;&#2361;&#2357;&#2366;&#2352;&#2368; &#2346;&#2352; &#2344;&#2395;&#2352; &#2352;&#2326;&#2375;&#2327;&#2366; &#2324;&#2352; &#2309;&#2346;&#2381;&#2346;&#2325;&#2375; &#2348;&#2340;&#2366;&#2319;&#2327;&#2366; &#2325;&#2368; &#2309;&#2346;&#2381;&#2346;&#2325;&#2368; &#2309;&#2327;&#2354;&#2368; &#2350;&#2366;&#2361;&#2357;&#2366;&#2352;&#2368; &#2325;&#2368; &#2340;&#2366;&#2352;&#2367;&#2326; &#2325;&#2381;&#2351;&#2366; &#2361;&#2376; |
