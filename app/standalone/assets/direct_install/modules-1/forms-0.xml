			</trigger>
		</group>
		<group ref="/data/overweight" appearance="field-list">
			<label ref="jr:itext('overweight-label')"/>
			<trigger ref="/data/overweight/overweight_counsel" appearance="minimal">
				<label ref="jr:itext('overweight_counsel-label')"/>
			</trigger>
		</group>
		<group ref="/data/obese" appearance="field-list">
