<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Home Visit</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/B32D74EA-A57D-423B-BC6D-51FD099BAD3D" uiVersion="1" version="1" name="Home Visit">
					<previously_sick/>
					<feeling_sick/>
					<clinic_visit_1/>
					<clinic_visit_2/>
					<previous_sickness/>
					<previous_clinic_visit_1/>
					<previous_clinic_visit_2/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><clinic_visit_1/><clinic_visit_2/><feeling_sick/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/><instance id="casedb" src="jr://instance/casedb"/>
			<bind nodeset="/data/previously_sick"/>
			<bind nodeset="/data/feeling_sick"/>
			<bind nodeset="/data/clinic_visit_1"/>
			<bind nodeset="/data/clinic_visit_2"/>
			<bind nodeset="/data/previous_sickness"/>
			<bind nodeset="/data/previous_clinic_visit_1"/>
			<bind nodeset="/data/previous_clinic_visit_2"/>
			<itext>
				<translation lang="en" default="">
					<text id="previously_sick-label">
						<value>Last time this woman reported she was sick! Ask her if she recovered</value>
					</text>
					<text id="feeling_sick-label">
						<value>Is the woman feeling sick today?</value>
					</text>
					<text id="clinic_visit_1-label">
						<value>Has woman been to clinic visit 1?</value>
					</text>
					<text id="clinic_visit_1-yes-label">
						<value>Yes</value>
					</text>
					<text id="clinic_visit_1-no-label">
						<value>No</value>
					</text>
					<text id="clinic_visit_2-label">
						<value>Has woman been to clinic visit 2?</value>
					</text>
					</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/clinic_visit_1" nodeset="/data/case/update/clinic_visit_1" relevant="count(/data/clinic_visit_1) &gt; 0"/><bind calculate="/data/clinic_visit_2" nodeset="/data/case/update/clinic_visit_2" relevant="count(/data/clinic_visit_2) &gt; 0"/><bind calculate="/data/feeling_sick" nodeset="/data/case/update/feeling_sick" relevant="count(/data/feeling_sick) &gt; 0"/><setvalue event="xforms-ready" ref="/data/previous_clinic_visit_1" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/clinic_visit_1"/><setvalue event="xforms-ready" ref="/data/previous_clinic_visit_2" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/clinic_visit_2"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<trigger ref="/data/previously_sick" appearance="minimal">
			<label ref="jr:itext('previously_sick-label')"/>
		</trigger>
		<select1 ref="/data/feeling_sick">
			<label ref="jr:itext('feeling_sick-label')"/>
			<item>
				<label ref="jr:itext('clinic_visit_1-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('clinic_visit_1-no-label')"/>
				<value>no</value>
			</item>
		</select1>
		<select1 ref="/data/clinic_visit_1">
			<label ref="jr:itext('clinic_visit_1-label')"/>
			<item>
				<label ref="jr:itext('clinic_visit_1-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('clinic_visit_1-no-label')"/>
				<value>no</value>
			</item>
		</select1>
		<select1 ref="/data/clinic_visit_2">
			<label ref="jr:itext('clinic_visit_2-label')"/>
			<item>
				<label ref="jr:itext('clinic_visit_1-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('clinic_visit_1-no-label')"/>
				<value>no</value>
			</item>
		</select1>
	</h:body>
</h:html>