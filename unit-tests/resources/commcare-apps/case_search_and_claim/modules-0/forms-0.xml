<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Registration</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/04F52515-B007-43E1-A8A3-CF9937CB55A5" uiVersion="1" version="1" name="Registration">
					<woman_name/>
					<village_name/>
					<lmp/>
					<show_edd/>
					<living_children/>
					<living_girls/>
					<living_boys/>
					<feeling_sick/>
					<clinic_reminder/>
					<total_children/>
					<edd/>
					<pregnancy_risk/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><create><case_name/><owner_id/><case_type>pregnancy</case_type></create><update><feeling_sick/><living_children/><lmp/><total_children/><village_name/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/woman_name" type="xsd:string" required="true()"/>
			<bind nodeset="/data/village_name" type="xsd:string" required="true()"/>
			<bind nodeset="/data/lmp" type="xsd:date" required="true()"/>
			<bind nodeset="/data/show_edd"/>
			<bind nodeset="/data/living_children"/>
			<bind nodeset="/data/living_girls" type="xsd:int" constraint=". &lt;= 15" jr:constraintMsg="jr:itext('living_girls-constraintMsg')" relevant="/data/living_children = 'yes'"/>
			<bind nodeset="/data/living_boys" type="xsd:int" constraint=". &lt;= 15" jr:constraintMsg="jr:itext('living_boys-constraintMsg')" relevant="/data/living_children = 'yes'"/>
			<bind nodeset="/data/feeling_sick"/>
			<bind nodeset="/data/clinic_reminder"/>
			<bind nodeset="/data/total_children" relevant="/data/living_girls + /data/living_boys"/>
			<bind nodeset="/data/edd" relevant="date(/data/lmp + 274)"/>
			<bind nodeset="/data/pregnancy_risk" relevant="if(/data/total_children &gt; 5 and /data/feeling_sick = 'yes','high_risk','low_risk')"/>
			<itext>
				<translation lang="en" default="">
					<text id="woman_name-label">
						<value>Woman's Names</value>
					</text>
					<text id="village_name-label">
						<value>Village name:</value>
					</text>
					<text id="lmp-label">
						<value>Date of Last Menstrual Period</value>
					</text>
					<text id="show_edd-label">
						<value>The woman's EDD is: <output value="/data/edd"/></value>
					</text>
					<text id="living_children-label">
						<value>Has the woman given birth to children that are still alive?</value>
					</text>
					<text id="living_girls-label">
						<value>How many living girls?</value>
					</text>
					<text id="living_girls-constraintMsg">
						<value>The number of living girls must be less than or equal to 15!</value>
					</text>
					<text id="living_boys-label">
						<value>How many living boys?</value>
					</text>
					<text id="living_boys-constraintMsg">
						<value>The number of living boys must be less than or equal to 15!</value>
					</text>
					<text id="feeling_sick-label">
						<value>Is the woman feeling sick today?</value>
					</text>
					<text id="feeling_sick-yes-label">
						<value>Yes</value>
					</text>
					<text id="feeling_sick-no-label">
						<value>No</value>
					</text>
					<text id="clinic_reminder-label">
						<value>Remind the pregnant woman to go to the clinic for her check-up!</value>
					</text>
				</translation>
			</itext>
      <bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/>
      <bind calculate="/data/woman_name" nodeset="/data/case/create/case_name"/><bind calculate="/data/meta/userID" nodeset="/data/case/create/owner_id"/><bind calculate="/data/feeling_sick" nodeset="/data/case/update/feeling_sick" relevant="count(/data/feeling_sick) &gt; 0"/><bind calculate="/data/living_children" nodeset="/data/case/update/living_children" relevant="count(/data/living_children) &gt; 0"/><bind calculate="/data/lmp" nodeset="/data/case/update/lmp" relevant="count(/data/lmp) &gt; 0"/><bind calculate="/data/total_children" nodeset="/data/case/update/total_children" relevant="count(/data/total_children) &gt; 0"/><bind calculate="/data/village_name" nodeset="/data/case/update/village_name" relevant="count(/data/village_name) &gt; 0"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<input ref="/data/woman_name">
			<label ref="jr:itext('woman_name-label')"/>
		</input>
		<input ref="/data/village_name">
			<label ref="jr:itext('village_name-label')"/>
		</input>
		<input ref="/data/lmp">
			<label ref="jr:itext('lmp-label')"/>
		</input>
		<trigger ref="/data/show_edd" appearance="minimal">
			<label ref="jr:itext('show_edd-label')"/>
		</trigger>
		<select1 ref="/data/living_children">
			<label ref="jr:itext('living_children-label')"/>
			<item>
				<label ref="jr:itext('feeling_sick-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('feeling_sick-no-label')"/>
				<value>no</value>
			</item>
		</select1>
		<input ref="/data/living_girls">
			<label ref="jr:itext('living_girls-label')"/>
		</input>
		<input ref="/data/living_boys">
			<label ref="jr:itext('living_boys-label')"/>
		</input>
		<select1 ref="/data/feeling_sick">
			<label ref="jr:itext('feeling_sick-label')"/>
			<item>
				<label ref="jr:itext('feeling_sick-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('feeling_sick-no-label')"/>
				<value>no</value>
			</item>
		</select1>
		<trigger ref="/data/clinic_reminder" appearance="minimal">
			<label ref="jr:itext('clinic_reminder-label')"/>
		</trigger>
	</h:body>
</h:html>
