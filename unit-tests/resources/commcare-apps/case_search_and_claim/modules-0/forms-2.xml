<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Birth Outcome</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/195E7292-A8F3-48B2-88DC-3471CD6F7606" uiVersion="1" version="1" name="Birth Outcome">
					<pregnancy_outcome/>
					<birth_weight/>
					<baby_name/>
					<dob/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><close/></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/pregnancy_outcome" required="true()"/>
			<bind nodeset="/data/birth_weight" type="xsd:double" relevant="/data/pregnancy_outcome = 'live_birth'"/>
			<bind nodeset="/data/baby_name" type="xsd:string" relevant="/data/pregnancy_outcome = 'live_birth'" required="true()"/>
			<bind nodeset="/data/dob" type="xsd:date" constraint=". &lt;= today()" jr:constraintMsg="jr:itext('dob-constraintMsg')" relevant="/data/pregnancy_outcome = 'live_birth'"/>
			<itext>
				<translation lang="en" default="">
					<text id="pregnancy_outcome-label">
						<value>Pregnancy Outcome:</value>
					</text>
					<text id="pregnancy_outcome-live_birth-label">
						<value>Live Birth</value>
					</text>
					<text id="pregnancy_outcome-still_birth-label">
						<value>Still Birth</value>
					</text>
					<text id="pregnancy_outcome-abortion-label">
						<value>Abortion</value>
					</text>
					<text id="birth_weight-label">
						<value>Weight at birth (kg):</value>
					</text>
					<text id="baby_name-label">
						<value>Child's Name:</value>
					</text>
					<text id="dob-label">
						<value>Date of Birth</value>
					</text>
					<text id="dob-constraintMsg">
						<value>Date of Birth cannot be in the future!</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<select1 ref="/data/pregnancy_outcome">
			<label ref="jr:itext('pregnancy_outcome-label')"/>
			<item>
				<label ref="jr:itext('pregnancy_outcome-live_birth-label')"/>
				<value>live_birth</value>
			</item>
			<item>
				<label ref="jr:itext('pregnancy_outcome-still_birth-label')"/>
				<value>still_birth</value>
			</item>
			<item>
				<label ref="jr:itext('pregnancy_outcome-abortion-label')"/>
				<value>abortion</value>
			</item>
		</select1>
		<input ref="/data/birth_weight">
			<label ref="jr:itext('birth_weight-label')"/>
		</input>
		<input ref="/data/baby_name">
			<label ref="jr:itext('baby_name-label')"/>
		</input>
		<input ref="/data/dob">
			<label ref="jr:itext('dob-label')"/>
		</input>
	</h:body>
</h:html>